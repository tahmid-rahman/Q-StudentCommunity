<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.text.TextFlow?>

<Button fx:id="topicButton" maxWidth="200.0" mnemonicParsing="false" onAction="#onTopicButtonClick" prefWidth="200.0" stylesheets="@../../../style/style.css" wrapText="true" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.q_studentcommunity.helpPopUp">
   <graphic>
      <TextFlow prefHeight="19.0" prefWidth="184.0">
         <children>
            <Text fx:id="topicText" strokeType="OUTSIDE" strokeWidth="0.0" text="Topic Name: " />
            <Text fx:id="buttonID" strokeType="OUTSIDE" strokeWidth="0.0" visible="false" />
         </children>
      </TextFlow>
   </graphic>
   <cursor>
      <Cursor fx:constant="OPEN_HAND" />
   </cursor>
   <styleClass>
      <String fx:value="help-popUp-hover" />
      <String fx:value="help-popUp-background" />
   </styleClass>
</Button>
